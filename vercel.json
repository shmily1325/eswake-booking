{
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/backup-to-drive",
      "schedule": "20 19 * * *"
    },
    {
      "path": "/api/backup-to-cloud-drive",
      "schedule": "0 23 30 * *"
    },
    {
      "path": "/api/line-reminder",
      "schedule": "0 19 * * *"
    }
  ]
}

